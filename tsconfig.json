{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "typeRoots": [
      "./types",
      "./client/types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "client/app.tsx",
    "./client/types",
  ],
  "exclude": [
    "./node_modules"
  ]
}